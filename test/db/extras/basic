NAME=r2hermes test
FILE=bins/hbc/bespoke_eval.hbc
ARGS=-e asm.sub.varmin=16
CMDS=<<EOF
-a
pd 2
EOF
EXPECT=<<EOF
hbc
            ;-- entry0:
            ;-- global_0x000000b0:
            0x000000b0      73000100       load_const_string r0, str.print_123__ ; 0xa2 ; "print(123);"
            0x000000b4      5e000000       direct_eval r0, r0, 0
EOF
RUN
